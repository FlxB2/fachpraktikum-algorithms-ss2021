{
  "openapi": "3.0.0",
  "info": {
    "title": "osm-tasks",
    "version": "0.1.0"
  },
  "paths": {
    "/build_graph": {
      "post": {
        "operationId": "build_graph",
        "responses": {
          "200": {
            "description": ""
          }
        }
      }
    },
    "/jobResult/{id}": {
      "get": {
        "operationId": "job_result",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "uint",
              "minimum": 0.0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ShipRoute"
                }
              }
            }
          },
          "404": {
            "description": ""
          }
        }
      }
    },
    "/jobStatus/{id}": {
      "get": {
        "operationId": "job_status",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "uint",
              "minimum": 0.0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "boolean"
                }
              }
            }
          }
        }
      }
    },
    "/route": {
      "post": {
        "operationId": "route",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RouteRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "integer",
                  "format": "uint32",
                  "minimum": 0.0,
                  "nullable": true
                }
              }
            }
          }
        }
      }
    },
    "/test_graph": {
      "get": {
        "operationId": "test",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "integer",
                  "format": "uint32",
                  "minimum": 0.0
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Node": {
        "type": "object",
        "required": [
          "lat",
          "lon"
        ],
        "properties": {
          "lat": {
            "type": "number",
            "format": "double"
          },
          "lon": {
            "type": "number",
            "format": "double"
          }
        }
      },
      "RouteRequest": {
        "type": "object",
        "required": [
          "end",
          "start"
        ],
        "properties": {
          "end": {
            "$ref": "#/components/schemas/Node"
          },
          "start": {
            "$ref": "#/components/schemas/Node"
          }
        }
      },
      "ShipRoute": {
        "type": "object",
        "required": [
          "nodes"
        ],
        "properties": {
          "nodes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Node"
            }
          }
        }
      }
    }
  }
}